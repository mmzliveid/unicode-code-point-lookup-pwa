{
  "hosting": [{
    "target": "unicode-converter",
    "public": "app",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**/!(*.@(js|html|css|json|svg|png|jpg|jpeg|xml|txt|ico))",
      "destination": "/index.html"
    }]
  }]
}
